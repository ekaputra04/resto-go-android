            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="182dp"
            tools:layout_editor_absoluteY="103dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24sp"
            android:fontFamily="@font/poppins_light"
            android:text="@string/tambahan"
            android:textColor="@color/gray"
            android:textSize="11sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="182dp"
            tools:layout_editor_absoluteY="155dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24sp"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/telur"
            android:textColor="@color/gray"
            android:textSize="12sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="182dp"
            tools:layout_editor_absoluteY="171dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24sp"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/rp_28000"
            android:textColor="@color/gray"
            android:textSize="12sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="182dp"
            tools:layout_editor_absoluteY="196dp" />

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginStart="284dp"
            android:layout_marginTop="196dp"
            android:background="@drawable/circle_background"
            android:scaleType="centerInside"
            android:src="@drawable/minus_mark"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24sp"
            android:fontFamily="@font/poppins_bold"
            android:text="@string/_2"
            android:textColor="@color/gray"
            android:textSize="16sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="321dp"
            tools:layout_editor_absoluteY="194dp" />

        <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:layout_marginLeft="344dp"
            android:layout_marginTop="196dp"
            android:background="@drawable/circle_background"
            android:scaleType="centerInside"
            android:src="@drawable/plus_mark"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/groupContainer2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="156dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="136dp"
            android:layout_height="117dp"
            android:background="@drawable/rounded_corners"
            android:padding="4dp"
            android:src="@drawable/nasi_tahu_bakar"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="103dp" />

        <TextView
            android:layout_width="126dp"
            android:layout_height="44dp"
            android:fontFamily="@font/poppins_semi_bold"
            android:text="Nasi Tahu Bakar"
            android:textColor="@color/white"
            android:textSize="18sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="182dp"
            tools:layout_editor_absoluteY="103dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="24sp"
            android:fontFamily="@font/poppins_light"
            android:text="@string/tambahan"
